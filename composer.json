{
  "name": "nwidart/laravel-menus",
  "description": "Laravel Menu management",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    },
    {
      "name": "Pingpong Labs",
      "email": "pingpong.labs@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "7.*",
    "illuminate/config": "7.*",
    "illuminate/view": "7.*",
    "laravelcollective/html": "7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "mockery/mockery": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Nwidart\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nwidart\\Menus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "7.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Nwidart\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "Nwidart\\Menus\\Facades\\Menu"
      }
    }
  }
}
